{
    "version": 2,
    "name": "project_deployment",
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}
